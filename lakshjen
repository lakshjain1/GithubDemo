pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                git 'https://github.com/pritam-khergade/student-ui.git'
            }
        }
        stage('install Maven') {
            steps {
                sh'''
                sudo dnf install maven -y
                sudo mvn clean install
                '''
            }
        }
        
}
}
